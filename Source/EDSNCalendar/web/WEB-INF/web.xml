<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <display-name>GetEvents</display-name>
        <servlet-name>GetEvents</servlet-name>
    </servlet>
    <servlet>
        <servlet-name>GetUnpublishedEvents</servlet-name>
        <servlet-class>GetUnpublishedEvents</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RejectionSender</servlet-name>
        <servlet-class>RejectionSender</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EventSubmit</servlet-name>
        <servlet-class>EventSubmit</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CalendarInsert</servlet-name>
        <servlet-class>CalendarInsert</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetEvents</servlet-name>
        <url-pattern>/GetEvents</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetUnpublishedEvents</servlet-name>
        <url-pattern>/GetUnpublishedEvents</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RejectionSender</servlet-name>
        <url-pattern>/RejectionSender</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EventSubmit</servlet-name>
        <url-pattern>/EventSubmit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CalendarInsert</servlet-name>
        <url-pattern>/CalendarInsert</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
